{
  "name": "URIjs",
  "version": "1.13.1",
  "title": "URI.js - Mutating URLs",
  "author": {
    "name": "Rodney Rehm",
    "url": "http://rodneyrehm.de"
  },
  "repository" :
  {
    "type" : "git",
    "url" : "https://github.com/medialize/URI.js.git"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license"
    },
    {
      "type": "GPL",
