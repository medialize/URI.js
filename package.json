    "query string",
    "RFC 3986",
    "RFC3986",
    "RFC 6570",
    "RFC6570"
  ],
  "categories": [
    "Parsers & Compilers",
    "Utilities"
  ],
  "main": "./src/URI",
  "homepage": "http://medialize.github.io/URI.js/",
  "contributors": [
    "Francois-Guillaume Ribreau <npm@fgribreau.com> (http://fgribreau.com)"
  ],
  "files": [
    "src/URI.js",
    "src/IPv6.js",
    "src/SecondLevelDomains.js",
    "src/punycode.js",
    "src/URITemplate.js",
    "src/jquery.URI.js",
    "src/URI.min.js",
    "src/jquery.URI.min.js",
    "src/URI.fragmentQuery.js",
    "src/URI.fragmentURI.js"
  ],
  
  "jam": {
    "main": "src/URI.js",
    "dependencies": {
      "jquery": ">=1.7.0"
    },
    "include": [
      "src/URI.js",
      "src/IPv6.js",
      "src/SecondLevelDomains.js",
      "src/punycode.js",
      "src/URITemplate.js",
      "src/jquery.URI.js",
      "src/URI.fragmentQuery.js",
      "src/URI.fragmentURI.js"
    ]
  },
  
  "npmName": "URIjs",
  "npmFileMap": [
    {
      "basePath": "/src/",
      "files": [
        "*.js"
      ]
    }
  ],
  "devDependencies": {
    "jshint-stylish": "~0.1.5",
    "grunt": "~0.4.2",
    "grunt-contrib-jshint": "~0.8.0"
  },
  "spm": {
    "main": "src/URI.js"
  }
